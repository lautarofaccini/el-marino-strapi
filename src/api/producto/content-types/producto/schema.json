{
  "kind": "collectionType",
  "collectionName": "productos",
  "info": {
    "singularName": "producto",
    "pluralName": "productos",
    "displayName": "Producto",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "nombre": {
      "type": "string",
      "required": true
    },
    "slug": {
      "type": "uid",
      "required": true,
      "targetField": "nombre"
    },
    "isActive": {
      "type": "boolean",
      "default": true,
      "required": false
    },
    "precio": {
      "type": "decimal",
      "required": true
    },
    "categorias": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::categoria.categoria",
      "inversedBy": "productos"
    },
    "descripcion": {
      "type": "blocks"
    },
    "imagenes": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "stock": {
      "type": "integer",
      "required": true,
      "min": 0
    }
  }
}
